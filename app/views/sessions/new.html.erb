<h1>Please Login or Signup</h1>

<%= form_tag sessions_path do %>
	<%= label_tag :username, "Username:" %>
	<%= text_field_tag :username %>
	<%= label_tag :password, "Password:" %>
	<%= password_field_tag :password %>
	<%= submit_tag "login" %>	
<% end %>

<%= link_to "Signup", new_applicant_path %>